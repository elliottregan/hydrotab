{
  "name": "Hydrotab",
  "version": "0.0.3",
  "background": {
    "page": "background.html"
  },
  "permissions": ["storage", "topSites"],
  "manifest_version": 2,
  "icons": {
    "16": "wave-16.png",
    "48": "wave-48.png",
    "128": "wave-128.png"
  },
  "browser_action": {
    "default_icon": "wave.png",
    "default_title": "Hydrotab",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_ui": {
    "page": "index.html",
    "open_in_tab": true
  }
}
